#!/usr/bin/expect -f

set DEVICE [lindex $argv 0]

# Defini le temps avant d'envoyer le mot de passe
set timeout 3
 
spawn ssh -p 5212 gothdroid@gothdroid.com
interact

expect "#"

system "cd /data/opendelta/last/$DEVICE\n"
system "rm -rf *.zip\n"
expect "#"
exit


spawn scp -P 5212 /data/opendelta/last/$DEVICE/*zip  gothdroid@gothdroid.com:/data/opendelta/last/$DEVICE/
expect "100%"
exit 0
